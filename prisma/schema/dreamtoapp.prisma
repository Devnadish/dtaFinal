model contactus {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String
  mobile    String
  message   String
  createdAt DateTime @default(now())
}

model freeOffer {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String
  mobile    String
  createdAt DateTime @default(now())
}
